<?xml version="1.0"?>

<launch>
  
  <!--<param name="/use_sim_time" value="true" />
 -->
 
  <node pkg="mav_tracker" type="mav_tracker_node" name="mav_tracker_node" output="screen">
    <remap from="~/image" to="/pseye_camera/image_raw"/>
    <remap from="~/camera_info" to="/pseye_camera/camera_info"/>
  </node>
   
  <node name="image_view_dial_debug" type="image_view" pkg="image_view" args="image:=/dial_gauge_detector_node/debug_image_array" output="screen" respawn="true" />
  <node name="image_view_dial_image_left" type="image_view" pkg="image_view" args="image:=/arm_stereo/left/image_raw" output="screen" respawn="true" />

</launch>    
